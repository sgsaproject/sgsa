<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.0" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
  <persistence-unit name="client-offlinePU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>model.TipoUsuario</class>
    <class>model.Permissao</class>
    <class>model.Usuario</class>
    <class>model.Sessao_1</class>
    <class>model.Ouvinte_1</class>
    <class>model.Palestra_1</class>
    <class>model.Ouvinte</class>
    <class>model.Palestra</class>
    <class>model.Sessao</class>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:derby://localhost:1527/sacta"/>
      <property name="javax.persistence.jdbc.password" value="sacta"/>
      <property name="javax.persistence.jdbc.driver" value="org.apache.derby.jdbc.ClientDriver"/>
      <property name="javax.persistence.jdbc.user" value="sacta"/>
    </properties>
  </persistence-unit>
</persistence>
